{
    "name": "bobfridley/workflowmax",
    "description": "A WorkFlowMax bridge for Laravel 5",
    "keywords": ["laravel", "framework", "workflowmax", "Bob Fridley", "BobFridley"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Fridley",
            "email": "robert.fridley@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*",
        "graham-campbell/manager": "^2.3"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "BobFridley\\WorkFlowMax\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BobFridley\\Tests\\WorkFlowMax\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
