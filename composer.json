{
    "name": "bobfridley/vonage",
    "description": "A Vonage bridge for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "vonage",
        "Bob Fridley",
        "BobFridley"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Fridley",
            "email": "bobfridley@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/contracts": "^5.2.8",
        "illuminate/support": "^5.2.8",
        "graham-campbell/manager": "^3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.4",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "BobFridley\\Vonage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BobFridley\\Tests\\Vonage\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
